{
  "name": "vchat-sites",
  "version": "1.0.0",
  "description": "",
  "author": "Dmitriy Petrov <floplopru@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=5478 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.2",
    "@nestjs/core": "^7.5.2",
    "@nestjs/mongoose": "^7.0.3",
    "@nestjs/platform-express": "^7.5.2",
    "@typegoose/typegoose": "7.4.2",
    "discord.js": "^12.5.1",
    "mongoose": "5.10.14",
    "nestjs-dotenv": "^1.2.1",
    "nestjs-rmq": "^2.0.5",
    "nestjs-typegoose": "^7.1.38",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.2",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^25.2.3",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^14.14.7",
    "concurrently": "^5.3.0",
    "jest": "^25.5.4",
    "prettier": "^2.1.2",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "4.0.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
